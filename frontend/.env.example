# Travel Ruter Frontend Environment Variables
# Copy this file to .env and fill in the values

# Backend API URL
VITE_API_URL=http://localhost:8000/api/v1

# Mapbox Access Token
# Get one at https://account.mapbox.com/auth/signup/
VITE_MAPBOX_TOKEN=your_mapbox_token_here

# Enable OAuth authentication (set to 'true' to require login)
VITE_AUTH_ENABLED=false
